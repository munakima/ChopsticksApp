@model IEnumerable<Models.HomePic>
@{
    ViewBag.Title = "Home Page";
}


<div class="container1" style="margin-top: 30px;">
    <div class="image-slider-wrapper">
        <ul id="image_slider" class="transition-delay">
            @foreach (var pic in Model)
            {
                <li><img style="width:1200px;height:800px" src="@pic.Url"></li>
            }

        </ul>
    </div>
    <div id="fb-root"></div>
    <script>
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.5";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));</script>
    <div class="fb-like" data-href="https://www.facebook.com/pages/Chopsticks-House/496699020386749?fref=ts" data-layout="button" data-action="like" data-show-faces="true" data-share="true"></div>

</div>